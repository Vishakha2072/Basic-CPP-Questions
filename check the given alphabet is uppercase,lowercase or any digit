#include <iostream>
using namespace std;
//check the given alphabet is uppercase,lowercase or any digit
int main() {
    char ch;
    cin>>ch;
    if(ch>='A' && ch<='Z')
    {
        cout<<"This is uppercase"<<endl;
    }
    else if(ch>='a' && ch<='z')
    {
        cout<<"This is lowercase"<<endl;
    }
    else if(ch>='0' && ch<='9')
    {
        cout<<"This is digit"<<endl;
    }
    return 0;
}
